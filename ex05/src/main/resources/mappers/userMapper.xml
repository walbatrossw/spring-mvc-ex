<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.doubles.ex05.mapper.UserMapper">

    <select id="login" resultType="UserVO">
        SELECT
            uid
            , upw
            , uname
        FROM tbl_user
        WHERE
            uid = #{uid}
            AND upw = #{upw}
    </select>

</mapper>